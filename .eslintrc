{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb, gatsby-standard"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react, import, jsx-a11y, react-hooks"],
  "parser": "babel-eslint"
}
